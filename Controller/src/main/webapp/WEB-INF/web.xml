<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Home</servlet-name>
  	<display-name>Home</display-name>
  	<description></description>
  	<servlet-class>org.studyeasy.Home</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DatabaseConfig</servlet-name>
  	<display-name>DatabaseConfig</display-name>
  	<description></description>
  	<servlet-class>org.studyeasy.config.DatabaseConfig</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>User</servlet-name>
  	<display-name>User</display-name>
  	<description></description>
  	<servlet-class>org.studyeasy.entity.User</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UsersModel</servlet-name>
  	<display-name>UsersModel</display-name>
  	<description></description>
  	<servlet-class>org.studyeasy.model.UsersModel</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Home</servlet-name>
  	<url-pattern>/Home</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DatabaseConfig</servlet-name>
  	<url-pattern>/DatabaseConfig</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>User</servlet-name>
  	<url-pattern>/User</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UsersModel</servlet-name>
  	<url-pattern>/UsersModel</url-pattern>
  </servlet-mapping>
</web-app>
